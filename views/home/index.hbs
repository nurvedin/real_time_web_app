  <div class="row">
    <div class="col">
      <h3>Issues</h3>
      {{#each viewData}}
        <ul>
          <div class="issue-container">
            <p>Title: {{this.title}}</p>
            <p>Url: {{this.url}}</p>
            <p>Number of comments: {{this.comments}}</p>
            <p>Creted at: {{this.created_at}}</p>
          </div>
        </ul>
      {{/each}}
    </div>
    <div class="col" id="notes">
      <h3>Notifications</h3> 
    </div>
  </div>